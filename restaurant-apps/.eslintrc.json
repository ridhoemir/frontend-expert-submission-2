{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": 0,
        "eslint-disable linebreak-style": 0,
        "no-console": 0,
        "no-restricted-globals": 0
    }
}
